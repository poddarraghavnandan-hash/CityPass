{
  "name": "citypass",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "postinstall": "pnpm db:generate",
    "dev": "turbo run dev --parallel",
    "build": "pnpm db:generate && turbo run build",
    "lint": "turbo run lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:quick": "vitest run --reporter=dot --no-coverage",
    "test:coverage": "vitest run --coverage",
    "db:generate": "pnpm --filter @citypass/db generate",
    "db:push": "pnpm --filter @citypass/db push",
    "db:migrate": "pnpm --filter @citypass/db migrate:dev",
    "db:studio": "pnpm --filter @citypass/db studio",
    "jobs:embed": "tsx scripts/embed-events.ts",
    "jobs:learn": "tsx scripts/learn-weights.ts",
    "jobs:reconcile": "tsx scripts/reconcile-budgets.ts",
    "jobs:cold-start": "tsx scripts/batch-cold-start.ts",
    "jobs:all": "pnpm jobs:embed && pnpm jobs:learn && pnpm jobs:reconcile",
    "deploy:setup": "tsx scripts/setup-vercel-env.ts",
    "deploy:preview": "vercel",
    "deploy:prod": "vercel --prod",
    "deploy:env": "vercel env pull .env.vercel"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.8",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "openai": "^6.8.1",
    "pg": "^8.16.3"
  }
}
