{
  "buildCommand": "cd ../.. && pnpm turbo run build --filter=@citypass/web",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/cron/seed-events",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/cron/scrape-events",
      "schedule": "0 */6 * * *"
    }
  ]
}
